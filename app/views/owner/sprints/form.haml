!!!
%h1= "Create new sprint"
%fieldset
  %legend Sprint details
  %body
    = form_for @sprint, :action => "/owner/projects/#{@project.pk}/sprint/create" do |f|
      %fieldset
        = f.select :day, :options => ("1".."31").to_a
        = f.select :month, :options => [["1", "january"], ["2", "february"], ["3", "march"], ["4", "april"], ["5", "may"], ["6", "june"], ["7", "july"], ["8", "august"], ["9", "september"], ["10", "october"], ["11", "november"], ["12", "december"]]
        = f.select :year, :options => ("2010".."2050").to_a
        = f.select :duration, :options => ("1".."7").to_a 
        %select{:name => "userstories[]", :multiple => true}
          -@user_stories.each do |u|
            %option{:value => u.pk}= u.title
        = f.submit "Submit"




