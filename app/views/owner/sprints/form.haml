!!!
%h1= "Create new sprint"
%fieldset
  %legend Sprint details
  %body
    = form_for @sprint, :action => "/owner/projects/#{@project.pk}/sprint_create" do |f|
      %fieldset
        = f.select :days, :options => ("0".."31").to_a
        = f.select :month, :options => {"1" => "january", "2" => "february", "3" => "march", "4" => "april", "5" => "may", "6" => "june", "7" => "july", "8" => "august", "9" => "september", "10" => "october", "11" => "november", "12" => "december"}
        = f.select :year, :options => ("2010".."2050").to_a
        = f.input :duration, :label => 'Duration'
        = f.submit "Submit"


